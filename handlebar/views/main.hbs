{{> header}}
<br>




<h2>Lista de productos</h2>

{{#if listExists}}
    <table>
        <tr> 
              <td><strong>Producto</strong></td>
              <td><strong>Precio </strong></td>
        
          
        </tr>
        
        {{#each suggestedChamps}}
            <tr>
                <td >{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            </tr>
            
            
        {{/each}}
    </table>
{{/if}}